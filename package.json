{
  "name": "@depaulisystems/dp-icons",
  "Author": "T. Cem Yilmaz",
  "version": "2.0.0-beta.6",
  "description": "Icon pack for DePauli AG",
  "main": "./dist/dp-icons.umd.js",
  "module": "./dist/dp-icons.es.js",
  "exports": {
    ".": {
      "import": "./dist/dp-icons.es.js",
      "require": "./dist/dp-icons.umd.js"
    }
  },
  "jsnext:main": "dist/index.es.js",
  "private": false,
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "prepublishOnly": "cross-env NODE_ENV=production node ./tools/converter.js",
    "build": "vite build",
    "publish": "node tools/converter.js && lerna bootstrap && lerna run --scope @depaulisystems/dp-icons build && lerna publish --yes --force-publish"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "cross-env": "^7.0.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.0",
    "pascal-case": "^3.1.2",
    "svg-path-converter": "^1.0.12",
    "svgo": "^2.3.0",
    "vite": "^2.9.0",
    "vue": "^3.2.31",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "css": "^3.0.0",
    "lerna": "^4.0.0",
    "svg-slim": "^2.0.3",
    "svgson": "^5.2.1"
  }
}
