{
  "name": "@depaulisystems/glyph",
  "Author": "T. Cem Yilmaz",
  "version": "1.0.4",
  "description": "Icon pack for DePauli AG",
  "main": "./dist/Glyph.umd.js",
  "module": "./dist/Glyph.es.js",
  "exports": {
    "./react": {
      "import": "./dist/GlyphReact.es.js",
      "require": "./dist/GlyphReact.umd.js"
    },
    "./vue": {
      "import": "./dist/GlyphVue.es.js",
      "require": "./dist/GlyphVue.umd.js"
    },
    "./bold": {
      "import": "./dist/GlyphBold.es.js",
      "require": "./dist/GlyphBold.umd.js"
    },
    "./regular": {
      "import": "./dist/GlyphRegular.es.js",
      "require": "./dist/GlyphRegular.umd.js"
    },
    "./misc": {
      "import": "./dist/GlyphMisc.es.js",
      "require": "./dist/GlyphMisc.umd.js"
    }
  },
  "jsnext:main": "dist/Glyph.es.js",
  "private": false,
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "release": "standard-version && git push --follow-tags origin fe && npm publish"
  },
  "standard-version": {
    "scripts": {
      "prerelease": "cross-env NODE_ENV=production node ./tools/converter.js && node build.mjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "cross-env": "^7.0.3",
    "css": "^3.0.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.0",
    "pascal-case": "^3.1.2",
    "standard-version": "^9.5.0",
    "svg-path-converter": "^1.0.12",
    "svg-slim": "^2.0.3",
    "svgo": "^2.3.0",
    "svgson": "^5.2.1",
    "vite": "^3.2.5",
    "vite-plugin-dts": "^1.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vue": "^3.2.26"
  }
}
